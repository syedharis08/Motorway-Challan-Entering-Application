<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="semester.project.GenderStatisticsController">
   <children>
      <BarChart fx:id="chart" layoutX="4.0" prefHeight="400.0" prefWidth="594.0" title="Gender Ratio">
        <xAxis>
          <CategoryAxis fx:id="numberofchalan" label="Gender" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="numbers" label="Number of Chalans" side="LEFT" />
        </yAxis>
      </BarChart>
   </children>
</AnchorPane>
